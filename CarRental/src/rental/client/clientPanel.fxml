              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                     <children>
                        <Label fx:id="rzeszowLabel" layoutX="25.0" layoutY="63.0" onMouseClicked="#menuItem_Clicked" prefHeight="17.0" prefWidth="158.0" styleClass="menuItem" stylesheets="@../resources/style.css" text="Rzeszów" />
