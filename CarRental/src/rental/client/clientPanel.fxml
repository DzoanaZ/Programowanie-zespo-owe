                  </AnchorPane>
            </content>
               <font>
                  <Font name="Caviar Dreams Bold" size="14.0" />
               </font>
          </TitledPane>
          <TitledPane fx:id="applyOrderTab" animated="false" styleClass="menu_font" text="Nowe wypożyczenia">
            <content>
              <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="508.0" prefWidth="200.0">
                     <children>
                        <Label fx:id="submitOrder" layoutX="16.0" layoutY="5.0" onMouseClicked="#menuItem_Clicked" prefHeight="28.0" prefWidth="174.0" styleClass="menuItem" stylesheets="@../resources/style.css" text="Złóż zamówienie" />
                     </children>
                  </AnchorPane>
            </content>
               <font>
                  <Font name="Caviar Dreams Bold" size="14.0" />
               </font>
          </TitledPane>
        </panes>
