                        <Label fx:id="myPersonalDataLabel" layoutX="23.0" layoutY="11.0" onMouseClicked="#menuItem_Clicked" prefHeight="17.0" prefWidth="160.0" styleClass="menuItem" stylesheets="@../resources/style.css" text="Dane osobowe">
